<div>
    {% for user in users %}
        <h2>ID: {{ user.id }}</h2>
        <h2>{{ user.name }}</h2>
        <h2>{{ user.last_name }}</h2>
        <h2>{{ user.login }}</h2>
        <h2>{{ user.email }}</h2>
        <h2>{{ user.is_admin }}</h2>
            <hr>
            <hr>
    {% else %}
        No user have been found.
    {% endfor %}

</div>